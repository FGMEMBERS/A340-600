<!-- Gear Animation -->

<!--       Left Gear      -->

    <animation>
        <type>rotate</type>
        <object-name>left_gear</object-name>
        <property>gear/gear[1]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>80</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>6.407</x-m>
            <y-m>-5.616</y-m>
            <z-m>-1.262</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

<!-- Right Gear -->
    <animation>
        <type>rotate</type>
        <object-name>right_gear</object-name>
        <property>gear/gear[2]/position-norm</property>
