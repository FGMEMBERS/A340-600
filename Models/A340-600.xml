<!-- Takeoff Engine Moisture -->

<model>
    <name>Left</name>
    <path>Aircraft/A340-600/Models/effects/exhaust.xml</path>
     
	 <condition>
	 <and>
           <greater-than>
            <property>position/altitude-agl-ft</property>
            <value>1</value>
          </greater-than>
	  
	  <and>
	  <greater-than>
                <property>environment/relative-humidity</property>
                <value>45</value>
            </greater-than>
	  
	  
	  <and>
	  <less-than-equals>
            <property>position/altitude-agl-ft</property>
            <value>75</value>
          </less-than-equals>
	  
	  <and>
	  <less-than-equals>
				<property>/sim/time/sun-angle-rad</property>
				<value>1.63</value>
			</less-than-equals>
			<and>
	  <greater-than>
                <property>engines/engine[0]/rpm</property>
                <value>90</value>
            </greater-than>
	    
	    <not>
   <property>sim/crashed</property>
   </not>
	    </and>
	    </and>
            </and>
	    </and>
	    </and>
	    </condition>
      
    <offsets>
  <x-m>-11.0</x-m>
  <y-m>-8.77</y-m>
  <z-m>-2.4</z-m>
      <pitch-deg>0</pitch-deg>
