	<!-- Animation for the logo light -->
	<animation>
		<name>Logo light illumination</name>
		<type>material</type>
		<object-name>rudder</object-name>
		<object-name>vstabs</object-name>
		<object-name>vstabp</object-name>
		<condition>
			<equals>
				<property>controls/lighting/nav-lights-switch</property>
				<value>2</value>
			</equals>
		</condition>
		<emission>
			<red>0.25</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>

