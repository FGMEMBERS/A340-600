<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList include="A346-main.xml">

	<sim>
		<description>Airbus A340-600</description>
		<flight-model>jsb</flight-model>
		<aero>A340-600</aero>
		<engine>RRT500</engine>
		<model>
			<path>Aircraft/A340-600/XMLs/A340-600.xml</path>
		<livery>
  		<file type="string"/>
 		</livery>
		</model>

		<tutorials include="Tutorials/a340-tutorials.xml" />

		<flaps>
			<setting>0.00</setting>
			<setting>0.25</setting>
			<setting>0.375</setting>
			<setting>0.5</setting>
			<setting>0.875</setting>
		</flaps>

		<rendering>
			<redout>
				<enabled type="bool">true</enabled>
				<parameters>
				  <blackout-complete-g type="double">7</blackout-complete-g>
				  <blackout-onset-g type="double">4.5</blackout-onset-g>
				  <redout-complete-g type="double">-4.5</redout-complete-g>
				  <redout-onset-g type="double">-2</redout-onset-g>
				</parameters>
			</redout>
			<camera-group>
				<znear type="double">0.1</znear>
			</camera-group>
		</rendering>
        
		<view>
			<name>Pilot View</name>
			<internal archive="y">true</internal>
			<config>
				<x-offset-m archive="y">-0.55</x-offset-m>
				<y-offset-m archive="y">0.8</y-offset-m>
				<z-offset-m archive="y">-34.35</z-offset-m>
				<pitch-offset-deg archive="y">-15.0</pitch-offset-deg>
<default-field-of-view-deg type="double">70.0</default-field-of-view-deg>
			</config>
		</view>
		<view n="100">
			<name>Gear View</name>
			<type>lookfrom</type>
			<internal archive="y">false</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0</x-offset-m>
				<y-offset-m archive="y">-3.0</y-offset-m>
				<z-offset-m archive="y">8.5</z-offset-m>
				<pitch-offset-deg archive="y">0</pitch-offset-deg>
			</config>
		</view>

  <view n="101">
    <name>Wing View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">70</default-field-of-view-deg>
      <pitch-offset-deg>-8.0</pitch-offset-deg>
      <heading-offset-deg type="double">90</heading-offset-deg> <!-- parallel to the panel -->
      <x-offset-m archive="y">-2.50 </x-offset-m> <!--Left/Right-->
      <y-offset-m archive="y"> 0.86 </y-offset-m> <!--Up/Down-->
      <z-offset-m archive="y">-2.74 </z-offset-m> <!--Fdw/Back-->
    </config>
  </view>
  
  <view n="102">
   <name>Tail view</name>
   <type>lookfrom</type>
   <internal type="bool">false</internal>
   <config>
    <from-model type="bool">true</from-model>
    <from-model-idx type="int">0</from-model-idx>
    <ground-level-nearplane-m type="double">6f</ground-level-nearplane-m>
    <x-offset-m type="double">0.00</x-offset-m>
    <y-offset-m type="double">12.00</y-offset-m>
    <z-offset-m type="double">36.51</z-offset-m>
    <pitch-offset-deg>-10</pitch-offset-deg>
    <default-field-of-view-deg type="double">65.0</default-field-of-view-deg>
   </config>
   <dynamic>
    <enabled type="bool">false</enabled>
   </dynamic>
  </view>


  <view n="103">
        <name>Copilot View</name>
        <type>lookfrom</type>
        <internal archive="y">true</internal>
        <config>
            <from-model type="bool">true</from-model>
            <x-offset-m archive="y">0.55</x-offset-m>
            <y-offset-m archive="y">0.8</y-offset-m>
            <z-offset-m archive="y">-34.35</z-offset-m>
            <pitch-offset-deg archive="y">-15.0</pitch-offset-deg>
        </config>
  </view>
        
  <view n="104">
        <name>Passenger View (Economy Class)</name>
        <type>lookfrom</type>
        <internal archive="y">true</internal>
        <config>
            <from-model type="bool">true</from-model>
            <x-offset-m archive="y">-1.77</x-offset-m>
            <y-offset-m archive="y">0.90</y-offset-m>
            <z-offset-m archive="y">14.66</z-offset-m>
        </config>
  </view>
        
  <view>
    <name>Distant Chase View</name>
    <type>lookat</type>
    <internal archive="y">false</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <at-model type="bool">true</at-model>
      <at-model-idx type="int">0</at-model-idx>

      <x-offset-m type="double">0</x-offset-m>
      <y-offset-m type="double">0</y-offset-m>
      <z-offset-m type="double">200</z-offset-m>
    </config>
  </view>

		<menubar>
			<default>
				<menu n="100">
					<label>Airbus A340-600</label>
				</menu>
			</default>
		</menubar>

		<multiplay>
			<generic>
				<!-- Default -->
			</generic>
		</multiplay>
	</sim>

<!-- Animated Jetways compatibility -->
	<aircraft>
		<door n="0">
			<x-m>16.339</x-m>
			<y-m>9.083</y-m>
			<z-m>4.494</z-m>
		</door>
		<door n="1">
			<x-m>30.996</x-m>
			<y-m>8.676</y-m>
			<z-m>4.496</z-m>
		</door>
		<jetway-hood-deg type="double">3</jetway-hood-deg>
	</aircraft>

	<payload>
		<weight n="1">
			<min-lb type="double">0</min-lb>
			<max-lb type="double">137345</max-lb>
		</weight>
	</payload>

	<instrumentation>
		<cdu>
			<ident>
				<model>A340-600</model>
				<engines>RRT500</engines>
			</ident>
		</cdu>
	</instrumentation>

</PropertyList>
